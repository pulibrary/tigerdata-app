            <xs:enumeration value="The MIT License" xml:lang="en">
                <xs:annotation>
                    <xs:documentation xml:lang="en">MIT</xs:documentation>
                    <xs:documentation xml:lang="en">https://opensource.org/license/MIT</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
